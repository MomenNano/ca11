{
  "name": "ca11",
  "private": true,
  "version": "0.8.0",
  "description": "User- and privacy-friendly FOSS communication software",
  "engines": {
    "node": ">=13.0.0",
    "yarn": ">= 1.19.0"
  },
  "scripts": {
    "build": "gulp -f packages/webphone/gulpfile.js build",
    "link": "npm link @garage11/ca11-theme-default;npm link @garage11/vue-compiler-gulp; npm link @garage11/vue-i18n",
    "lint": "npm run eslint;npm run lint:scss",
    "lint:js": "eslint '{packages/**/*.js,gulpfile.js,gulp/**/*.js,tests/**/*.js}'",
    "lint:scss": "stylelint 'packages/**/*.scss'",
    "release": "standard-version",
    "test:e2e": "gulp test-browser",
    "test:release": "standard-version --dry-run",
    "test:unit": "gulp test-unit",
    "test": "npm run test-unit;npm run test-lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.js": "eslint",
    "**/*.scss": "stylelint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/garage11/ca11"
  },
  "keywords": [
    "communication",
    "decentralized",
    "p2p",
    "sip",
    "softphone",
    "vue",
    "webrtc"
  ],
  "author": "Garage 11",
  "main": "main.js",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/garage11/ca11/issues"
  },
  "devDependencies": {
    "@commitlint/cli": "7.x.x",
    "@commitlint/config-conventional": "7.x.x",
    "@garage11/vue-compiler-gulp": "1.x.x",
    "ansi-colors": "4.x.x",
    "browserify": "16.x.x",
    "connect": "3.x.x",
    "connect-mount": "1.x.x",
    "cz-conventional-changelog": "^2.1.0",
    "del": "2.x.x",
    "eslint": "6.x.x",
    "eslint-plugin-sort-class-members": "1.x.x",
    "glob": "7.x.x",
    "gulp": "4.x.x",
    "gulp-add-src": "1.x.x",
    "gulp-clean-css": "4.x.x",
    "gulp-cli": "2.x.x",
    "gulp-concat": "2.x.x",
    "gulp-envify": "1.x.x",
    "gulp-eslint": "6.x.x",
    "gulp-filter": "6.x.x",
    "gulp-flatten": "0.x.x",
    "gulp-if-else": "1.x.x",
    "gulp-imagemin": "6.x.x",
    "gulp-insert": "0.x.x",
    "gulp-livereload": "4.x.x",
    "gulp-notify": "3.x.x",
    "gulp-rename": "1.x.x",
    "gulp-sass": "4.x.x",
    "gulp-sentry-release-manager": "0.x.x",
    "gulp-size": "2.x.x",
    "gulp-sourcemaps": "2.x.x",
    "gulp-stylelint": "10.x.x",
    "gulp-svgo": "2.x.x",
    "gulp-tape": "^1.x.x",
    "gulp-template": "5.x.x",
    "gulp-uglify": "3.x.x",
    "gulplog": "1.x.x",
    "husky": "3.x.x",
    "lerna": "^3.19.0",
    "lint-staged": "9.x.x",
    "mkdirp": "0.x.x",
    "puppeteer": "2.x.x",
    "rc": "^1.2.8",
    "standard-version": "7.x.x",
    "stylelint": "12.x.x",
    "stylelint-order": "3.x.x",
    "stylelint-scss": "3.x.x",
    "tap-spec": "5.x.x",
    "tape": "4.x.x",
    "tape-catch": "1.x.x",
    "through2": "3.x.x",
    "tildify": "2.x.x",
    "vinyl-buffer": "1.x.x",
